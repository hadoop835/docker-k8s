[Unit]
Description=Kubernetes Proxy
After=network.target
[Service]
EnvironmentFile=-/config
EnvironmentFile=-/kube-proxy
ExecStart=/kube-proxy                         ${KUBE_LOGTOSTDERR}                     ${KUBE_LOG_LEVEL}                       ${NODE_HOSTNAME}                        ${KUBE_MASTER}                          ${KUBE_PROXY_ARGS}
Restart=on-failure
[Install]
WantedBy=multi-user.target
